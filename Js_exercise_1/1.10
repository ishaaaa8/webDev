function intersection(a,b){
    let arr=[];
    for(let i=0;i<a.length;i++){
        if(b.includes(a[i])){
            if(!arr.includes(a[i])){
                arr.push(a[i]);
            }
        }
    }
    arr.sort((x,y)=>x-y);
    return arr;
}

const a=[1,2,3,4,5];

const b=[3,4,5,6,7];

const commonEle=intersection(a,b);



console.log(commonEle);
